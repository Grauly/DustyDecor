{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "Dusty Decor",
  "description": "Adds various more or less decor in the rough theme of what you would find in dust covered, abandoned spaces.",
  "authors": [
    "Grauly"
  ],
  "contact": {
    "homepage": "none, haha",
    "sources": "https://github.com/grauly/DustyDecor"
  },
  "license": "CC0-1.0",
  "icon": "assets/dustydecor/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "grauly.dustydecor.DustyDecorMod",
        "adapter": "kotlin"
      }
    ],
    "client": [
      {
        "value": "grauly.dustydecor.DustyDecorClient",
        "adapter": "kotlin"
      }
    ],
    "fabric-datagen": [
      {
        "value": "grauly.dustydecor.DustyDecorDatagen",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "dustydecor.mixins.json",
    {
      "config": "dustydecor.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "~1.21",
    "java": ">=21",
    "fabric-api": "*",
    "fabric-language-kotlin": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}