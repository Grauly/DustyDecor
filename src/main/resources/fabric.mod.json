{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "Template Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/dustydecor/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "grauly.dustydecor.DustyDecorMod",
        "adapter": "kotlin"
      }
    ],
    "client": [
      {
        "value": "grauly.dustydecor.DustyDecorClient",
        "adapter": "kotlin"
      }
    ],
    "fabric-datagen": [
      {
        "value": "grauly.dustydecor.DustyDecorDatagen",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "dustydecor.mixins.json",
    {
      "config": "dustydecor.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.21",
    "java": ">=21",
    "fabric-api": "*",
    "fabric-language-kotlin": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}